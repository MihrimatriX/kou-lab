@{
    ViewBag.Title = "Zaman Atla";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2 class="page-header"><i class="fa fa-clock-o"></i>Zaman Atla</h2>

<div class="form">
    <form class="form-validate form-horizontal" id="feedback_form" method="post" action="/Admin/ZamanAtla" enctype="multipart/form-data">
        <div class="form-group">
            <label class="control-label col-sm-4">Gidilecek Zamanı Girin<span class="required">*</span></label>
            <div class="col-sm-6">
                <input id="dp1" type="date" name="GetirmeTarih" value="Atlanacak Günü Giriniz." size="16" class="form-control" required>
            </div>
        </div>

        <div class="form-group">
            <div class="col-lg-offset-2 col-lg-10">
                <button class="btn btn-primary" type="submit">Kaydet</button>
                <button class="btn btn-default" type="button"><a href="/Admin/Index/">Geri Dön</a></button>
            </div>
        </div>
    </form>
</div>